    {
      "title": "Enter the address of where your business records are kept",
      "includeIf": "${accountsAddressChoice = 2}",
      "fields": [
        {
          "id": "recAccAdd",
          "type": "address",
          "label": ""
        }
      ]
    },
    {
      "title": "Do you want to register one or more premises for Gaming Duty? ",
      "fields": [
        {
          "id": "gd58Choice",
          "type": "choice",
          "label": " ",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Enter the number of new premises you want to register",
      "includeIf": "${gd58Choice = 0}",
      "fields": [
        {
          "id": "newPremNumb",
          "type": "text",
          "label": "",
          "format": "positiveWholeNumber"
        }
      ]
    },
    {
      "title": "Enter the details of premises $n",
