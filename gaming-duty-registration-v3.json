        {
          "id": "premName",
          "type": "text",
          "label": "Trading name "
        },
        {
          "id": "premAdd",
          "type": "address",
          "label": "Address "
        },
        {
          "id": "premTel",
          "type": "text",
          "label": "Telephone number at this premises",
          "format": "telephoneNumber"
        }
      ]
    },
    {
      "title": "What date will gaming start at the premises you are registering?",
      "fields": [
        {
          "id": "startTradingDate",
          "type": "date",
          "label": "",
          "helpText": "DD MM YYYY"
        }
      ]
    },
    {
      "title": "Do you want to add a partner to the Gaming Duty register?",
      "fields": [
        {
          "type": "choice",
          "id": "Yes",
          "label": "",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "How many partners would you like to register?",
      "includeIf": "${legalStatus1 = 2}",
      "fields": [
        {
          "id": "numberPartners",
          "type": "text",
          "label": "",
          "format": "positiveWholeNumber"
        }
      ]
    },
    {
      "title": "Enter details of partner $n",
      "repeatsMin": "${numberPartners}",
      "repeatsMax": "${numberPartners}",
      "fields": [
        {
          "id": "PartnerFirstName",
          "type": "text",
          "label": "First name"
        },
        {
          "id": "PartnerLastName",
          "type": "text",
          "label": "Last name"
        },
        {
          "id": "PartnerAddress",
          "type": "address",
          "label": "Address"
        },
        {
          "id": "PartnerTel",
          "type": "text",
          "label": "Telephone number",
          "format": "telephoneNumber"
        }
      ]
    },
    {
      "title": "Do you want to set up a new Gaming Duty group?",
      "fields": [
        {
          "type": "choice",
          "id": "newGroup",
          "label": "Do you want to set up a new Gaming Duty Group?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
