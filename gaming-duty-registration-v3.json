          "label": "Full name representative member"
        },
        {
          "type": "text",
          "id": "",
          "label": "Enter the full name of group member number $n"
        },
        {
          "type": "info",
          "id": "generatedId_a517d5f4_42bc_448c_acbe_d783ac852888",
          "label": "",
          "infoText": "Add new member",
          "infoType": "noformat"
        }
      ]
    },
    {
      "title": "Is each member of the group, including the representative member, a member of a corporate group?",
      "fields": [
        {
          "id": "corpMembChoice",
          "type": "choice",
          "label": " ",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Choice field: Is each member of the group, including the representative member, either resident or established within the UK or Isle of Man?",
      "fields": [
        {
          "id": "membResiChoice",
          "type": "choice",
          "label": "Is each member of the group, including the representative member, either resident or established within the UK or Isle of Man?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    }
