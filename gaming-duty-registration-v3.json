  "sections": [
    {
      "title": "Enter your details",
      "fields": [
        {
          "id": "generatedId_052dc509_0c21_4b28_b306_4252a094ba7b",
          "type": "info",
          "label": "",
          "infoText": "We will only use your email address and phone number to contact you if we have a question.",
          "infoType": "noformat"
        },
        {
          "id": "firstName",
          "type": "text",
          "label": "First name",
          "mandatory": "true"
        },
        {
          "id": "lastName",
          "type": "text",
          "label": "Last name",
          "mandatory": "true"
        },
        {
          "id": "Telephone number",
          "type": "text",
          "label": "Telephone number "
        },
        {
          "id": "Email address",
          "type": "text",
          "label": "Email address (optional)"
        },
        {
          "id": "capacity",
          "type": "text",
          "label": "What is your role within the organisation?",
          "helpText": "For example, a director or proprietor"
        }
      ]
    },
    {
      "title": "What type of business do you want to register?",
      "fields": [
        {
          "id": "legalStatus1",
          "type": "choice",
          "label": "",
          "shortName": "Business legal status",
          "format": "vertical",
          "choices": [
            "Limited company",
            "Sole proprietorship",
            "Partnership",
            "Unincorporated body"
          ]
        }
      ]
    },
    {
      "title": "Enter your full name",
      "includeIf": "${legalStatus1 = 1}",
      "fields": [
        {
          "id": "spName",
          "type": "text",
          "label": ""
        },
        {
          "id": "spNameChoice",
          "type": "choice",
          "label": "Is your trading name different to your full name?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "spTradingName",
          "type": "text",
          "label": "Trading name"
        }
      ]
    },
    {
      "title": "Enter the name of your partnership",
      "includeIf": "${legalStatus1 = 2}",
      "fields": [
        {
          "id": "pName",
          "type": "text",
          "label": ""
        },
        {
          "id": "nameChoicePA",
          "type": "choice",
          "label": "Is your trading name different to your partnership's name?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "pTradingName",
          "type": "text",
          "label": "Trading name"
        }
      ]
    },
    {
      "title": "Enter the name of your company",
      "includeIf": "${legalStatus1 = 0}",
      "fields": [
        {
          "id": "coName",
          "type": "text",
          "label": ""
        },
        {
          "id": "nameChoiceLC",
          "type": "choice",
          "label": "Is your trading name different from your company's name?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "tradingName",
          "type": "text",
          "label": "Trading name"
        }
      ]
    },
    {
      "title": "Enter the name of your unincorporated body",
      "includeIf": "${legalStatus1 = 4}",
      "fields": [
        {
          "id": "coNameUB",
          "type": "text",
          "label": ""
        },
        {
          "id": "nameChoiceUB",
          "type": "choice",
          "label": "Is your trading name different from your unincorporated body's name?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "tradingNameUB",
          "type": "text",
          "label": "Trading name"
        }
      ]
    },
    {
      "title": "Enter an estimate of your profits over the next 12 months",
      "fields": [
        {
          "id": "estProf",
          "type": "text",
          "label": "",
          "helpText": "You might call your 'profits'  your ‘gross gaming yield’ ",
          "format": "sterling"
        }
      ]
    },
    {
      "title": "Enter the address of your principal place of business",
