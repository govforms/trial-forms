        {
          "id": "ppbAdd",
          "type": "address",
          "label": ""
        },
        {
          "id": "ppbTel",
          "type": "text",
          "label": "Business' telephone number",
          "helpText": "You must always include the area or dialling code",
          "format": "telephoneNumber"
        },
        {
          "id": "accountsAddressChoice",
          "type": "choice",
          "label": "Are your business records stored at this address?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
