{
  "_id": "gaming-duty-registration-v2",
  "formName": "Register for Gaming Duty",
  "description": "",
  "emailTemplateId": "eeitt_submission_confirmation",
  "authConfig": {
    "authModule": "hmrc"
  },
  "dmsSubmission": {
    "businessArea": "For example: FinanceOpsCorpT",
    "classificationType": "For example: BT-NRU-Environmental",
    "customerId": "${fieldIdOfCustomerIdentifier}",
    "dmsFormId": "GD56"
  },
  "sections": [
    {
      "title": "About you",
      "fields": [
        {
          "id": "firstName",
          "type": "text",
          "label": "First name",
          "mandatory": "true"
        },
        {
          "id": "lastName",
          "type": "text",
          "label": "Last name",
          "mandatory": "true"
        },
        {
          "id": "capacity",
          "type": "text",
          "label": "Capacity in which you are completing this form",
          "helpText": "For example, a director or proprietor"
        }
      ]
    },
    {
      "title": "Contact details",
      "fields": [
        {
          "id": "personalTelephone",
          "type": "text",
          "label": "Telephone number",
          "format": "telephoneNumber"
        },
        {
          "id": "personalEmail",
          "type": "text",
          "label": "Email address",
          "format": "email"
        }
      ]
    },
    {
      "title": "Business details",
      "fields": [
        {
          "id": "legalStatus1",
          "type": "choice",
          "label": "Legal status of the business",
          "shortName": "Business legal status",
          "format": "vertical",
          "choices": [
            "Limited company",
            "Sole proprietor",
            "Partnership",
            "Group",
            "Unincorporated body"
          ]
        }
      ]
    },
    {
      "title": "Sole proprietor details",
      "includeIf": "${legalStatus1 = 1}",
      "fields": [
        {
          "id": "spNameChoice",
          "type": "choice",
          "label": "Is your trading name different to your full name?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "saNumberChoice",
          "type": "choice",
          "label": "Do you have a Self Assessment number?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "groupChoiceSP",
          "type": "choice",
          "label": "Are you the representative member of a group for Gaming Duty purposes?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Partnership details",
      "includeIf": "${legalStatus1 = 2}",
      "fields": [
        {
          "id": "nameChoicePA",
          "type": "choice",
          "label": "Is the trading name different to your full name?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "corpTaxRefChoice",
          "type": "choice",
          "label": "Do you have a Corporation Tax reference?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "groupChoicePA",
          "type": "choice",
          "label": "Are you the representative member of a group for Gaming Duty purposes?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Limited company",
      "includeIf": "${legalStatus1 = 0}",
      "fields": [
        {
          "id": "coName",
          "type": "text",
          "label": "Registered company name"
        },
        {
          "id": "tradingName",
          "type": "text",
          "label": "Trading name"
        },
        {
          "id": "nameChoiceLC",
          "type": "choice",
          "label": "Is the trading name different from the company name?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "corpTaxRefChoiceLC",
          "type": "choice",
          "label": "Do you have a Corporation Tax reference?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "groupChoiceLC",
          "type": "choice",
          "label": "Are you the representative member of a group for Gaming Duty purposes?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Unincorporated body",
      "includeIf": "${legalStatus1 = 4}",
      "fields": [
        {
          "id": "coNameUB",
          "type": "text",
          "label": "Company name"
        },
        {
          "id": "tradingNameUB",
          "type": "text",
          "label": "Trading name"
        },
        {
          "id": "nameChoiceUB",
          "type": "choice",
          "label": "Is the trading name different from the company name?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "corpTaxRefChoiceUB",
          "type": "choice",
          "label": "Do you have a Corporation Tax reference?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "groupChoiceUB",
          "type": "choice",
          "label": "Are you the representative member of a group for Gaming Duty purposes?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "What is your Self Assessment Unique Taxpayer Reference?",
      "includeIf": "${saNumberChoice = 0}",
      "fields": [
        {
          "id": "saUTR",
          "type": "text",
          "label": "",
          "helpText": "This is 10 numbers and will be on tax returns and other letters about Self Assessment. It may be called ‘reference’, ‘UTR’ or ‘official use’",
          "format": "utr"
        }
      ]
    },
    {
      "title": "Trading name",
      "includeIf": "${(spNameChoice = 0 || nameChoicePA = 1) || (nameChoiceLC = 0 || nameChoiceUB = 0)}",
      "fields": [
        {
          "id": "spTradingName",
          "type": "text",
          "label": "Trading name"
        }
      ]
    },
    {
      "title": "What is your Corporation Tax Unique Taxpayer Reference?",
      "includeIf": "${(corpTaxRefChoice = 0 || corpTaxRefChoiceLC = 0) || corpTaxRefChoiceUB = 0}",
      "fields": [
        {
          "id": "corpTaxRef",
          "type": "text",
          "label": "",
          "helpText": "This is 10 numbers and will be on tax returns and other letters about Corporation Tax. It may be called ‘reference’, ‘UTR’ or ‘official use’"
        }
      ]
    },
    {
      "title": "Your principal place of business",
      "fields": [
        {
          "id": "ppbAdd",
          "type": "address",
          "label": "Address",
          "international": "true"
        },
        {
          "id": "ppbTel",
          "type": "text",
          "label": "Telephone number",
          "helpText": "You must always include the area or dialling code",
          "format": "telephoneNumber"
        },
        {
          "id": "premAddressChoice",
          "type": "choice",
          "label": "Is this address the same as your Gaming Duty premises?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "accountsAddressChoice",
          "type": "choice",
          "label": "Is this the same address where your records of accounts are stored?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Do you keep your records and accounts at your principal place of business?",
      "fields": [
        {
          "id": "recAddChoice",
          "type": "choice",
          "label": "",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Where are your records and accounts kept?",
      "includeIf": "${recAddChoice = 0}",
      "fields": [
        {
          "id": "recAccAdd",
          "type": "address",
          "label": ""
        }
      ]
    },
    {
      "title": "Do you want to register more than one premises for Gaming Duty? ",
      "fields": [
        {
          "id": "gd58Choice",
          "type": "choice",
          "label": " ",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Enter the number of new premises you want to register",
      "includeIf": "${gd58Choice = 0}",
      "fields": [
        {
          "id": "newPremNumb",
          "type": "text",
          "label": "",
          "format": "positiveWholeNumber"
        }
      ]
    },
    {
      "title": "About the business - premises $n",
      "includeIf": "${gd58Choice = 0}",
      "repeatsMin": "${newPremNumb}",
      "repeatsMax": "${newPremNumb}",
      "fields": [
        {
          "id": "premName",
          "type": "text",
          "label": "Trading name of the premises"
        },
        {
          "id": "premAdd",
          "type": "address",
          "label": "Address of the premises"
        },
        {
          "id": "premTel",
          "type": "text",
          "label": "Telephone number",
          "format": "telephoneNumber"
        }
      ]
    },
    {
      "title": "How many partners would you like to register?",
      "includeIf": "${legalStatus1 = 1}",
      "fields": [
        {
          "id": "numberPartners",
          "type": "text",
          "label": "",
          "format": "positiveWholeNumber"
        }
      ]
    },
    {
      "title": "Partner $n details",
      "repeatsMin": "${numberPartners}",
      "repeatsMax": "${numberPartners}",
      "fields": [
        {
          "id": "PartnerFirstName",
          "type": "text",
          "label": "First name"
        },
        {
          "id": "PartnerLastName",
          "type": "text",
          "label": "Last name"
        },
        {
          "id": "PartnerAddress",
          "type": "address",
          "label": "Address"
        },
        {
          "id": "PartnerTel",
          "type": "text",
          "label": "Telephone number",
          "format": "telephoneNumber"
        }
      ]
    },
    {
      "title": "What date should group treatment start?",
      "includeIf": "${groupChoiceUB = 0 || groupChoiceLC = 0 || groupChoicePA = 0 || groupChoiceSP = 0 } ",
      "fields": [
        {
          "id": "newGroupStartDate",
          "type": "date",
          "label": ""
        }
      ]
    },
    {
      "title": "Enter group representation details",
      "fields": [
        {
          "id": "nameRN",
          "type": "text",
          "label": "Full name of representative member"
        },
        {
          "id": "corpMembChoice",
          "type": "choice",
          "label": "Is each member of the group, including the representative member, a member of a corporate group?",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "membResiChoice",
          "type": "choice",
          "label": "Is each member of the group, including the representative member, either resident or established within the UK or Isle of Man?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "How many members are there in the group?",
      "fields": [
        {
          "id": "numberMembers",
          "type": "text",
          "label": "",
          "format": "positiveWholeNumber"
        }
      ]
    },
    {
      "title": "Enter the full name of group member number $n",
      "repeatsMin": "${numberMembers}",
      "repeatsMax": "${numberMembers}",
      "fields": [
        {
          "id": "memberFullName",
          "type": "text",
          "label": ""
        }
      ]
    },
    {
      "title": "Additional information",
      "fields": [
        {
          "id": "startTradingDate",
          "type": "date",
          "label": "When will the business start trading?",
          "helpText": "DD MM YYYY"
        },
        {
          "id": "estProf",
          "type": "text",
          "label": "What is your estimated profit for the next 12 months?",
          "format": "sterling"
        },
        {
          "id": "addInfoChoice",
          "type": "choice",
          "label": "Is there any additional information you need to add?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Enter the additional information",
      "fields": [
        {
          "id": "addInfoText",
          "type": "text",
          "label": "",
          "multiline": "true"
        }
      ]
    }
  ],
  "declarationSection": {
    "title": "Submit your application to register for Gaming Duty",
    "shortName": "Submit your application",
    "fields": [
      {
        "id": "declarationNameGroup",
        "type": "group",
        "label": "",
        "fields": [
          {
            "id": "declaration-firstname",
            "type": "text",
            "label": "First name",
            "errorMessage": "First name can only include letters a to z, numbers, spaces, hyphens and apostrophes"
          },
          {
            "id": "declaration-lastname",
            "type": "text",
            "label": "Last name",
            "errorMessage": "Last name can only include letters a to z, numbers, spaces, hyphens and apostrophes"
          },
          {
            "id": "declaration-status",
            "type": "text",
            "label": "Status",
            "helpText": "For example, director or financial controller or secretary",
            "errorMessage": "Position in the company can only include letters, numbers, spaces, hyphens and apostrophes"
          }
        ]
      },
      {
        "id": "declarationEmailGroup",
        "type": "group",
        "label": "Enter an email address to receive confirmation your application has been submitted",
        "fields": [
          {
            "id": "email",
            "type": "text",
            "label": "Email address",
            "format": "email",
            "mandatory": "true",
            "errorMessage": "Enter a valid email address with no spaces, like name@example.com"
          }
        ]
      },
      {
        "id": "decText",
        "type": "info",
        "label": "",
        "infoText": "By submitting this application, you declare that the information you have provided is true and complete.\n\nIf you give incomplete or inaccurate information, we may charge a financial penalty or even prosecute you."
      }
    ]
  },
  "acknowledgementSection": {
    "title": "Confirmation page",
    "shortName": "Acknowledgement Page",
    "fields": [
      {
        "id": "generatedId_17d4070a-efc2-44a4-90db-f6c40ad644e4",
        "type": "info",
        "label": "Get a copy of form",
        "infoText": "We’ve sent your application to HMRC.\n\nIf you want a copy of your application you'll need to print it or save it as a PDF before you leave this page. ",
        "infoType": "noformat"
      }
    ]
  },
  "submitErrorUrl": "deprecated",
  "submitSuccessUrl": "deprecated"
}