{
  "_id": "cash-control-declaration",
  "formName": "Cash Control Declaration",
  "description": "",
  "emailTemplateId": "eeitt_submission_confirmation",
  "authConfig": {
    "authModule": "anonymous"
  },
  "dmsSubmission": {
    "businessArea": "For example: FinanceOpsCorpT",
    "classificationType": "For example: BT-NRU-Environmental",
    "customerId": "${passportNumber}",
    "dmsFormId": "CCD"
  },
  "sections": [
    {
      "title": "Are you, or your family, carrying more than 10,000 euros total in cash into or out of the EU?",
      "fields": [
        {
          "id": "eligibilityEuros",
          "type": "choice",
          "label": "Are you, or your family, carrying more than 10,000 euros total in cash into or out of the EU?",
          "helpText": "This includes notes and coins, bankers drafts, cheques and travellers cheques, and in any currency equal to 10,000 euros.",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "generatedId_85027177_f711_4814_8ab0_734ca4054e5a",
          "type": "info",
          "label": "cashWarning",
          "infoText": "You could face a penalty of up to Â£5,000 if you do not declare your cash or give incorrect information.",
          "infoType": "important"
        }
      ]
    },
    {
      "title": "You do not need to make a cash declaration",
      "includeIf": "${eligibilityEuros=1}",
      "continueIf": "false",
      "fields": [
        {
          "id": "generatedId_a296d82f_c5e3_4348_9d8e_cbbcea29b5b1",
          "type": "info",
          "label": "",
          "infoText": "Place holder text",
          "infoType": "noformat"
        }
      ]
    },
    {
      "title": "About you",
      "fields": [
        {
          "id": "fullName",
          "type": "text",
          "label": "Full name"
        },
        {
          "id": "dateOfBirth",
          "type": "date",
          "label": "Date of birth",
          "helpText": "For example, 21 03 1979"
        },
        {
          "id": "placeOfBirth",
          "type": "text",
          "label": "Place of birth",
          "helpText": "For example, London"
        },
        {
          "id": "countryOfBirth",
          "type": "text",
          "label": "Country of birth"
        },
        {
          "id": "nationality",
          "type": "text",
          "label": "Nationality"
        },
        {
          "id": "occupation",
          "type": "text",
          "label": "Occupation"
        }
      ]
    },
    {
      "title": "Passport details",
      "fields": [
        {
          "id": "passportNumber",
          "type": "text",
          "label": "Passport number",
          "helpText": "For example, 502135326",
          "format": "text(0,20)"
        },
        {
          "id": "passportExpiryDate",
          "type": "date",
          "label": "Expiry date",
          "helpText": "For example, 31 3 1980"
        }
      ]
    },
    {
      "title": "Your address",
      "fields": [
        {
          "id": "yourAddress",
          "type": "address",
          "label": "Your address",
          "international": "true"
        }
      ]
    },
    {
      "title": "Are you entering or leaving the EU?",
      "fields": [
        {
          "id": "enterExit",
          "type": "choice",
          "label": "Are you entering or leaving the EU?",
          "choices": [
            "Entering",
            "Leaving"
          ]
        }
      ]
    },
    {
      "title": "When are you entering the EU?",
      "includeIf": "${enterExit=0}",
      "fields": [
        {
          "id": "entryDate",
          "type": "date",
          "label": "When are you entering the EU?"
        }
      ]
    },
    {
      "title": "When are you leaving the EU?",
      "includeIf": "${enterExit=1}",
      "fields": [
        {
          "id": "leaveDate",
          "type": "date",
          "label": "When are you leaving the EU?"
        }
      ]
    },
    {
      "title": "Are you travelling into the EU by air or by sea?",
      "fields": [
        {
          "id": "airSea",
          "type": "choice",
          "label": "Are you travelling into the EU by air or by sea?",
          "choices": [
            "Air",
            "Sea"
          ]
        }
      ]
    },
    {
      "title": "Where will you start your journey?",
      "fields": [
        {
          "id": "startName",
          "type": "text",
          "label": "Name of airport, port, or station"
        },
        {
          "id": "countryName",
          "type": "text",
          "label": "Country name"
        }
      ]
    },
    {
      "title": "Where will you end your journey?",
      "fields": [
        {
          "id": "endName",
          "type": "text",
          "label": "Name of final airport, port, or station"
        },
        {
          "id": "destinationCountry",
          "type": "text",
          "label": "Country name"
        }
      ]
    },
    {
      "title": "Did you travel via an intermediate airport, port or station?",
      "fields": [
        {
          "id": "intermediateAirport",
          "type": "choice",
          "label": "Did you travel via an intermediate airport, port or station?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "How many airports, ports or stations did you travel through?",
      "fields": [
        {
          "id": "howManyAirports",
          "type": "text",
          "label": "How many airports, ports or stations did you travel through?",
          "helpText": "These are the intermediate places you went between your start and end journey ",
          "format": "positiveWholeNumber",
          "displayWidth": "xs",
          "validIf": "${howManyAirports>0 && howManyAirports<=20}"
        }
      ]
    },
    {
      "title": "Which intermediate airports, ports or stations did you travel through?",
      "repeatsMin": "${howManyAirports}",
      "repeatsMax": "${howManyAirports}",
      "fields": [
        {
          "id": "intermediatePortName",
          "type": "text",
          "label": "Name of airport, port or station"
        },
        {
          "id": "intermediatePortCountryName",
          "type": "text",
          "label": "Name of country"
        }
      ]
    },
    {
      "title": "Where did you make this declaration?",
      "fields": [
        {
          "id": "declarationPlaceName",
          "type": "text",
          "label": "Name of airport, port, or station"
        },
        {
          "id": "declarationCountryName",
          "type": "text",
          "label": "Country name"
        }
      ]
    },
    {
      "title": "How many different currencies are you carrying?",
      "fields": [
        {
          "id": "howManyCurrency",
          "type": "text",
          "label": "How many different currencies are you carrying?",
          "helpText": "For example euros, GBP, USD",
          "displayWidth": "xs",
          "validIf": "${howManyCurrency>0 && howManyCurrency <=10}"
        }
      ]
    },
    {
      "title": "Currency $n",
      "repeatsMin": "${howManyCurrency}",
      "repeatsMax": "${howManyCurrency}",
      "fields": [
        {
          "id": "currencyType",
          "type": "text",
          "label": "Currency",
          "helpText": "For example, euros, GBP, USD"
        },
        {
          "id": "noteTotal",
          "type": "text",
          "label": "Total amount in notes",
          "helpText": "Enter 0 if not applicable",
          "format": "positiveNumber(9, 2)"
        },
        {
          "id": "totalCoins",
          "type": "text",
          "label": "Total amount in coins",
          "helpText": "Enter 0 if not applicable",
          "format": "positiveNumber(9, 2)"
        },
        {
          "id": "totalTravellersCheque",
          "type": "text",
          "label": "Total amount in travellers cheques",
          "helpText": "Enter 0 if not applicable",
          "format": "positiveNumber(9, 2)"
        },
        {
          "id": "totalBankersDraft",
          "type": "text",
          "label": "Total amount in bankers drafts",
          "helpText": "Enter 0 if not applicable",
          "format": "positiveNumber(9, 2)"
        }
      ]
    },
    {
      "title": "Does the money belong to you?",
      "fields": [
        {
          "id": "moneyOwner",
          "type": "choice",
          "label": "Does the money belong to you?",
          "helpText": "If you own more than 50% of the money, select yes",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Details of main cash owner",
      "includeIf": "${moneyOwner=1}",
      "fields": [
        {
          "id": "cashOwnerName",
          "type": "text",
          "label": "Full name"
        },
        {
          "id": "cashOwnerBusiness",
          "type": "text",
          "label": "Business or occupation"
        },
        {
          "id": "cashOwnerAddressUK",
          "type": "address",
          "label": "Main cash owner address",
          "international": "true"
        }
      ]
    },
    {
      "title": "Cash details",
      "description": "Provide details of where the cash comes from",
      "fields": [
        {
          "id": "cashOrigin",
          "type": "text",
          "label": "Cash origin",
          "helpText": "For example, sale of a property, personal savings or business takings"
        },
        {
          "id": "countryGenerated",
          "type": "text",
          "label": "Country origin"
        },
        {
          "id": "dateGenerated",
          "type": "date",
          "label": "Date",
          "helpText": "This is for 'when the cash is generated'"
        }
      ]
    },
    {
      "title": "Person receiving the cash",
      "fields": [
        {
          "id": "receivingName",
          "type": "text",
          "label": "Full name",
          "helpText": "If more than one person receiving the cash, enter the name of the person receiving the most"
        },
        {
          "id": "receivingOccupation",
          "type": "text",
          "label": "Business or occupation"
        },
        {
          "id": "receivingAddress",
          "type": "address",
          "label": "Address",
          "international": "true"
        }
      ]
    },
    {
      "title": "What is the person receiving the cash going to use it for?",
      "fields": [
        {
          "id": "cashIntent",
          "type": "text",
          "label": "What is the person receiving the cash going to use it for?",
          "helpText": "For example, buying a car",
          "multiline": "true"
        }
      ]
    }
  ],
  "declarationSection": {
    "title": "Declaration",
    "shortName": "Declaration",
    "fields": [
      {
        "id": "generatedId_127c7990_0ddd_4608_b134_f9ab7eaeb2c8",
        "type": "info",
        "label": "decText",
        "infoText": "By signing below, you confirm that:\n\n* you are bringing 10,000 Euros or more into the EU\n\n* the information in this form is true, complete and accurate",
        "infoType": "noformat"
      },
      {
        "id": "declarationNameGroup",
        "type": "group",
        "label": "Provide details below of who is signing the declaration",
        "fields": [
          {
            "id": "declarationFullName",
            "type": "text",
            "label": "Full name"
          }
        ]
      },
      {
        "id": "declarationEmail",
        "type": "text",
        "label": "Email address",
        "helpText": "We will only us this to send a confirmation that we have received your application"
      }
    ]
  },
  "acknowledgementSection": {
    "title": "Confirmation page",
    "shortName": "Acknowledgement Page",
    "fields": [
      {
        "id": "helpText8",
        "type": "info",
        "label": "",
        "infoText": "You need to save a copy of this form now, so you can show it to customs officers if they ask.\n\nThis is your last opportunity to print or save a pdf copy of your submitted form."
      }
    ]
  },
