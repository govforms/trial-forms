{
  "_id": "cash-control-declaration",
  "formName": "Cash Control Declaration",
  "description": "",
  "emailTemplateId": "eeitt_submission_confirmation",
  "emailParameters": [
    {
      "value": "${declarationFullName}",
      "emailTemplateVariable": "customerName"
    }
  ],
  "authConfig": {
    "authModule": "anonymous"
  },
  "dmsSubmission": {
    "businessArea": "For example: FinanceOpsCorpT",
    "classificationType": "For example: BT-NRU-Environmental",
    "customerId": "${passportNumber}",
    "dmsFormId": "CCD"
  },
  "sections": [
    {
      "title": "Are you, or your family, carrying more than £10,000 total in cash into or out of the UK?",
      "fields": [
        {
          "id": "eligibilityEuros",
          "type": "choice",
          "label": "Are you, or your family, carrying more than £10,000 total in cash into or out of the UK?",
          "helpText": "This includes notes and coins, bankers drafts, cheques and travellers cheques, and in any currency equal to £10,000",
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "generatedId_85027177_f711_4814_8ab0_734ca4054e5a",
          "type": "info",
          "label": "cashWarning",
          "infoText": "You could face a penalty of up to £5,000 if you do not declare your cash or give incorrect information.",
          "infoType": "important"
        }
      ]
    },
    {
      "title": "You do not need to make a cash declaration",
      "includeIf": "${eligibilityEuros=1}",
      "continueIf": "false",
      "fields": [
        {
          "id": "generatedId_a296d82f_c5e3_4348_9d8e_cbbcea29b5b1",
          "type": "info",
          "label": "",
          "infoText": "You told us that the cash you are carrying (including currency notes, travellers cheques and bank drafts) totals less than £10,000",
          "infoType": "noformat"
        }
      ]
    },
    {
      "title": "About you",
      "fields": [
        {
          "id": "fullName",
          "type": "text",
          "label": "Full name"
        },
        {
          "id": "dateOfBirth",
          "type": "date",
          "label": "Date of birth",
          "helpText": "For example, 21 03 1979"
        },
        {
          "id": "placeOfBirth",
          "type": "text",
          "label": "Place of birth",
          "helpText": "For example, London"
        },
        {
          "id": "countryOfBirth",
          "type": "text",
          "label": "Country of birth"
        },
        {
          "id": "nationality",
          "type": "text",
          "label": "Nationality"
        },
        {
          "id": "occupation",
          "type": "text",
          "label": "Occupation"
        }
      ]
    },
    {
      "title": "Passport details",
      "fields": [
        {
          "id": "passportNumber",
          "type": "text",
          "label": "Passport number",
          "helpText": "For example, 502135326",
          "format": "text(0,20)"
        },
        {
          "id": "passportExpiryDate",
          "type": "date",
          "label": "Expiry date",
          "helpText": "For example, 31 3 1980"
        }
      ]
    },
    {
      "title": "Your address",
      "fields": [
        {
          "id": "yourAddress",
          "type": "address",
          "label": "Your address",
          "international": "true"
        }
      ]
    },
    {
      "title": "Are you entering or leaving the UK?",
      "fields": [
        {
          "id": "enterExit",
          "type": "choice",
          "label": "Are you entering or leaving the UK?",
          "choices": [
            "Entering",
            "Leaving"
          ]
        }
      ]
    },
    {
      "title": "When are you entering the UK?",
      "includeIf": "${enterExit=0}",
      "fields": [
        {
          "id": "entryDate",
          "type": "date",
          "label": "When are you entering the UK?"
        }
      ]
    },
    {
      "title": "When are you leaving the UK?",
      "includeIf": "${enterExit=1}",
      "fields": [
        {
          "id": "leaveDate",
          "type": "date",
          "label": "When are you leaving the UK?"
        }
      ]
    },
    {
      "title": "Are you travelling into the UK by air or by sea?",
      "fields": [
        {
          "id": "airSea",
          "type": "choice",
          "label": "Are you travelling into the UK by air or by sea?",
          "choices": [
            "Air",
            "Sea",
            "Channel tunnel"
          ],
          "optionHelpText": [
            "",
            "This is via ferry ports, for example",
            ""
          ]
        }
      ]
    },
    {
      "title": "Where will you start your journey?",
      "fields": [
        {
          "id": "startName",
          "type": "text",
          "label": "Name of airport, port, or station"
        },
        {
          "id": "countryName",
          "type": "text",
          "label": "Country name"
        }
      ]
    },
    {
      "title": "Where will you end your journey?",
      "fields": [
        {
          "id": "endName",
          "type": "text",
          "label": "Name of airport, port, or station"
        },
        {
          "id": "destinationCountry",
          "type": "text",
          "label": "Country name"
        }
      ]
    },
    {
      "title": "Did you travel via another airport, port or station?",
      "fields": [
        {
          "id": "intermediateAirport",
          "type": "choice",
          "label": "Did you travel via another airport, port or station?",
          "helpText": "If you stopped and transferred to another plane, boat or train, select yes ",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "How many other airports, ports or stations did you travel via?",
      "includeIf": "${intermediateAirport=0}",
      "fields": [
        {
          "id": "howManyAirports",
          "type": "text",
          "label": "How many other airports, ports or stations did you travel via?",
          "format": "positiveWholeNumber",
          "displayWidth": "xs",
          "validIf": "${howManyAirports>0 && howManyAirports<=20}"
        }
      ]
    },
    {
      "title": "Which airports, ports or stations did you travel via in between your start and end points?",
      "repeatsMin": "${howManyAirports}",
      "repeatsMax": "${howManyAirports}",
      "fields": [
        {
          "id": "intermediatePortName",
          "type": "text",
          "label": "Name of airport, port or station"
        },
        {
          "id": "intermediatePortCountryName",
          "type": "text",
          "label": "Name of country"
        }
      ]
    },
    {
      "title": "Where did you make this declaration?",
      "description": "This is the entry or exit point in the UK",
      "fields": [
        {
          "id": "declarationPlaceName",
          "type": "text",
          "label": "Name of airport, port, or station"
        },
        {
          "id": "declarationCountryName",
          "type": "text",
          "label": "Country name"
        }
      ]
    },
    {
      "title": "How many different currencies are you carrying?",
      "fields": [
        {
          "id": "howManyCurrency",
          "type": "text",
          "label": "How many different currencies are you carrying?",
          "helpText": "For example euros, GBP, USD",
          "displayWidth": "xs",
          "validIf": "${howManyCurrency>0 && howManyCurrency <=10}"
        }
      ]
    },
    {
      "title": "Currency $n",
      "repeatsMin": "${howManyCurrency}",
      "repeatsMax": "${howManyCurrency}",
      "fields": [
        {
          "id": "currencyType",
          "type": "text",
          "label": "Currency",
          "helpText": "For example, euros, GBP, USD"
        },
        {
          "id": "noteTotal",
          "type": "text",
          "label": "Total amount in notes/coins",
          "helpText": "Enter 0 if not applicable",
          "format": "positiveNumber(9, 2)"
        },
        {
          "id": "totalTravellersCheque",
          "type": "text",
          "label": "Total amount in travellers cheques",
          "helpText": "Enter 0 if not applicable",
          "format": "positiveNumber(9, 2)"
        },
        {
          "id": "totalBankersDraft",
          "type": "text",
          "label": "Total amount in bankers drafts",
          "helpText": "Enter 0 if not applicable",
          "format": "positiveNumber(9, 2)"
        }
      ]
    },
    {
      "title": "Does the cash belong to you?",
      "fields": [
        {
          "id": "moneyOwner",
          "type": "choice",
          "label": "Does the cash belong to you?",
          "helpText": "If you own more than 50% of the cash, select yes",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Details of main cash owner",
      "includeIf": "${moneyOwner=1}",
      "fields": [
        {
          "id": "cashOwnerName",
          "type": "text",
          "label": "Full name"
        },
        {
          "id": "cashOwnerBusiness",
          "type": "text",
          "label": "Business or occupation"
        },
        {
          "id": "cashOwnerAddressUK",
          "type": "address",
          "label": "Main cash owner address",
          "international": "true"
        }
      ]
    },
    {
      "title": "Cash details",
      "description": "Provide details of where the cash comes from",
      "fields": [
        {
          "id": "cashOrigin",
          "type": "text",
          "label": "Cash origin",
          "helpText": "For example, sale of a property, personal savings or business takings"
        },
        {
          "id": "countryGenerated",
          "type": "text",
          "label": "Country origin"
        }
      ]
    },
    {
      "title": "Are you keeping the cash for your own use?",
      "fields": [
        {
          "id": "cashYours",
          "type": "choice",
          "label": "Are you keeping the cash for your own use?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "title": "Person receiving the cash",
      "includeIf": "${cashYours=1}",
      "fields": [
        {
          "id": "receivingName",
          "type": "text",
          "label": "Full name",
          "helpText": "If more than one person receiving the cash, enter the name of the person receiving the most"
        },
        {
          "id": "receivingOccupation",
          "type": "text",
          "label": "Business or occupation"
        },
        {
          "id": "receivingAddress",
          "type": "address",
          "label": "Address",
          "international": "true"
        }
      ]
    },
    {
      "title": "What will the cash be used for?",
      "fields": [
        {
          "id": "cashIntent",
          "type": "text",
          "label": "What will the cash be used for?",
          "helpText": "For example, buying a car, going on holiday or helping with living expenses",
          "multiline": "true"
        }
      ]
    }
  ],
  "declarationSection": {
    "title": "Declaration",
    "shortName": "Declaration",
    "fields": [
      {
        "id": "generatedId_127c7990_0ddd_4608_b134_f9ab7eaeb2c8",
        "type": "info",
        "label": "decText",
        "infoText": "By signing below, you confirm that:\n\n* you are bringing 10,000 Euros or more into the EU\n\n* the information in this form is true, complete and accurate",
        "infoType": "noformat"
      },
      {
        "id": "declarationNameGroup",
        "type": "group",
        "label": "Provide details below of who is signing the declaration",
        "fields": [
          {
            "id": "declarationFullName",
            "type": "text",
            "label": "Full name"
          },
          {
            "id": "declarationEmail",
            "type": "text",
            "label": "Email address",
            "helpText": "We will only us this to send a confirmation that we have received your application",
            "format": "email"
          }
        ]
      }
    ]
  },
  "acknowledgementSection": {
    "title": "Confirmation page",
    "shortName": "Acknowledgement Page",
    "fields": [
      {
        "id": "helpText8",
        "type": "info",
        "label": "",
        "infoText": "You need to save a copy of this form now, so you can show it to customs officers if they ask.\n\nThis is your last opportunity to print or save a pdf copy of your submitted form."
      }
    ]
  },
  "GFC579Ready": "true",
  "submitErrorUrl": "deprecated",
  "submitSuccessUrl": "deprecated"
}